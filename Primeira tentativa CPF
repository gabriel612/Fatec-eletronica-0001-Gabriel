//periodo das funções para o primeiro digito


float cpf_primeiro_digito
(float dig2, float dig3, 
 float dig4, float dig5,
 float dig6, float dig7,
 float dig8, float dig9, 
 float dig10)
{
int resto=(dig10*10)+(dig9*9)
    +(dig8*8)+(dig7*7)+
    (dig6*6)+(dig5*5)+
    (dig4*4)+(dig3*3)+
    (dig2*2);
    x=resto%11;
    res=11-x;

return res;
}

// periodo das funções para o segundo digito

float cpf_segundo_digito
(float dig2, float dig3, 
 float dig4, float dig5,
 float dig6, float dig7,
 float dig8, float dig9, 
 float dig10, float dig11)
{
int resto=(dig11*11)+(dig10*10)+(dig9*9)
    +(dig8*8)+(dig7*7)+
    (dig6*6)+(dig5*5)+
    (dig4*4)+(dig3*3)+
    (dig2*2);
    y=resto%11;
    res1=11-y;
return res1;
}
