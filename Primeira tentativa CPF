float cpf_primeiro_digito 
(float dig2, float dig3,
float dig4, float dig5,
float dig6, float dig7,
float dig8, float dig9,
float dig10);




float x=cpf_primeiro_digito(3,3,9,8,9,2,8,3,8);

    
    
    printf(" o primeiro digito eh:\n %1.f\t",res); 
    printf("\nsegundo digito eh:\n %1.f",res1); 



float cpf_segundo_digito
(float dig2, float dig3, 
 float dig4, float dig5,
